{
  "name": "react-async-flux",
  "version": "1.0.0",
  "description": "Boilerplate for async data flow using React and Redux",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_PATH=src/ babel-node src/server.js",
    "dev": "npm start & webpack --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aduth/react-async-flux.git"
  },
  "keywords": [
    "react",
    "async",
    "redux",
    "flux"
  ],
  "author": {
    "name": "Andrew Duthie",
    "email": "andrew@andrewduthie.com",
    "url": "http://www.andrewduthie.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aduth/react-async-flux/issues"
  },
  "homepage": "https://github.com/aduth/react-async-flux",
  "babel": {
    "stage": 0,
    "optional": [ "runtime" ]
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.1",
    "react": "^0.13.3",
    "react-redux": "^0.6.0",
    "redux": "^1.0.0-rc",
    "socket.io-client": "^1.3.6",
    "webpack": "^1.11.0"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.3",
    "socket.io": "^1.3.6"
  }
}
